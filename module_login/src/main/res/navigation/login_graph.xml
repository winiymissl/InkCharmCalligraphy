<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/login_graph"
    app:startDestination="@id/fragment_loginDestination_login">
    <fragment
        android:id="@+id/fragment_loginDestination_login"
        android:name="com.example.module_login.ui.login.LoginFragment">
        <action
            android:id="@+id/action_fragment_loginDestination_login_to_fragment_loginDestination_register"
            app:destination="@id/fragment_loginDestination_register" />
        <deepLink app:uri="android-app://android/login" />
    </fragment>
    <fragment
        android:id="@+id/fragment_loginDestination_register"
        android:name="com.example.module_login.ui.register.RegisterFragment" />
</navigation>